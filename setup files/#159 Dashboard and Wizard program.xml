<?xml version="1.0"?>
<SECA2-CONFIG>
<!-- Patch for #159-->
<!--start dashboard program-->
<PROGRAM>
	<PROGRAM_NAME>dashboard</PROGRAM_NAME>
	<PROGRAM_VIEWROOT>/programs/dashboard/layout.xhtml</PROGRAM_VIEWROOT>
	<PROGRAM_DISPLAY_NAME>Dashboard</PROGRAM_DISPLAY_NAME>
	<PROGRAM_DISPLAY_DESC>See all your activities.</PROGRAM_DISPLAY_DESC>
	<PROGRAM_PUBLIC>false</PROGRAM_PUBLIC>
</PROGRAM>
<PROGRAM_ASSIGN>
	<PROGRAM_ASSIGN_PROGRAM>dashboard</PROGRAM_ASSIGN_PROGRAM>
	<PROGRAM_ASSIGN_USERTYPE>Segmail Administrator</PROGRAM_ASSIGN_USERTYPE>
	<PROGRAM_ASSIGN_USERTYPE>Segmail User</PROGRAM_ASSIGN_USERTYPE>
</PROGRAM_ASSIGN>
<MENU>
	<MENU_NAME>Dashboard</MENU_NAME>
	<MENU_URL>/dashboard/</MENU_URL>
	<MENU_PREPEND>&lt;i class=&quot;fa fa-dashboard&quot;&gt;&lt;/i&gt;</MENU_PREPEND>
	<MENU_PUBLIC>false</MENU_PUBLIC>
</MENU>
<MENU_ASSIGN>
	<MENU_ASSIGN_USERTYPE>Segmail Administrator</MENU_ASSIGN_USERTYPE>
	<MENU_ASSIGN_USERTYPE>Segmail User</MENU_ASSIGN_USERTYPE>
	<MENU_ASSIGN_MENU>Dashboard</MENU_ASSIGN_MENU>
	<MENU_ASSIGN_ORDER>-100000</MENU_ASSIGN_ORDER>
	<MENU_ASSIGN_GROUP>LEFT</MENU_ASSIGN_GROUP>
</MENU_ASSIGN>
<!--end dashboard program-->


<!--start wizard program-->
<PROGRAM>
	<PROGRAM_NAME>wizard</PROGRAM_NAME>
	<PROGRAM_VIEWROOT>/programs/wizard/layout.xhtml</PROGRAM_VIEWROOT>
	<PROGRAM_DISPLAY_NAME>Wizard</PROGRAM_DISPLAY_NAME>
	<PROGRAM_DISPLAY_DESC>Basic setup guide to get you going.</PROGRAM_DISPLAY_DESC>
	<PROGRAM_PUBLIC>false</PROGRAM_PUBLIC>
</PROGRAM>
<PROGRAM_ASSIGN>
	<PROGRAM_ASSIGN_PROGRAM>wizard</PROGRAM_ASSIGN_PROGRAM>
	<PROGRAM_ASSIGN_USERTYPE>Segmail Administrator</PROGRAM_ASSIGN_USERTYPE>
	<PROGRAM_ASSIGN_USERTYPE>Segmail User</PROGRAM_ASSIGN_USERTYPE>
</PROGRAM_ASSIGN>
<!--no need menu-->

<!--end dashboard program-->

</SECA2-CONFIG>